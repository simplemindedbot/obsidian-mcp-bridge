# Example Environment Variables for Development
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local or any file containing real secrets

# Web Search API Keys
BRAVE_API_KEY=your-brave-api-key-here
SERP_API_KEY=your-serp-api-key-here

# Database Connection Strings
DATABASE_URL=postgresql://user:password@localhost:5432/mydb
SQLITE_DB_PATH=/path/to/your/database.db

# External Service Tokens
GITHUB_TOKEN=ghp_your-github-token-here
NOTION_API_KEY=secret_your-notion-integration-token
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token

# LLM API Keys (if using external LLM services)
OPENAI_API_KEY=sk-your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Custom MCP Server Authentication
CUSTOM_SERVER_AUTH_TOKEN=your-custom-server-token
CUSTOM_SERVER_SECRET=your-server-secret

# Development Flags
DEBUG_MODE=true
LOG_LEVEL=debug